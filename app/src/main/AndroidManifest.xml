<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beachwatchers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_bird"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_bird_round"
        android:supportsRtl="true"
        android:theme="@style/BWatchersTheme">
        <activity
            android:name=".WSplash"
            android:theme="@style/BWatch_Action_Bar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WLogin"
            android:theme="@style/BWatch_Action_Bar"/>
        <activity android:name=".WMainActivity"></activity>
    </application>

</manifest>